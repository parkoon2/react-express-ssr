{
    "name": "react-ssr",
    "version": "1.0.0",
    "description": "Server side rendering project",
    "main": "index.js",
    "scripts": {
        "dev": "npm-run-all --parallel dev:*",
        "dev:server": "nodemon --watch build --exec node build/server.bundle.js",
        "dev:watch-server": "cross-env NODE_ENV=development  webpack --config webpack.server.js --watch",
        "dev:watch-client": "cross-env NODE_ENV=development webpack --config webpack.client.js --watch",
        "build": "npm-run-all --parallel build:*",
        "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.js",
        "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
        "deploy": "npm i && npm run build && node build/server.bundle.js"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@babel/node": "^7.7.7",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/polyfill": "^7.7.0",
        "@babel/preset-env": "^7.7.7",
        "@babel/preset-react": "^7.7.4",
        "babel-loader": "^8.0.6",
        "babel-polyfill": "^6.26.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^6.0.3",
        "css-loader": "^3.4.1",
        "file-loader": "^5.0.2",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "ignore-loader": "^0.1.2",
        "isomorphic-style-loader": "^5.1.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^7.0.0",
        "nodemon": "^2.0.2",
        "npm-run-all": "^4.1.5",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.1.2",
        "terser-webpack-plugin": "^2.3.1",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.5",
